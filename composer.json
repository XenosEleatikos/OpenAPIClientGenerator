{
    "name": "xenos/openapi-client-generator",
    "type": "library",
    "description": "A PHP code generator for OpenAPI clients",
    "keywords": ["OpenAPI", "swagger", "json-schema", "php", "api", "client", "library"],
    "homepage": "https://github.com/XenosEleatikos/OpenAPIClientGenerator",
    "license": "MIT",
    "authors": [
        {
            "name": "Christoph Hochholzer",
            "email": "christoph@hochholzer.info",
            "homepage": "https://github.com/XenosEleatikos",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=8.2.0",
        "psr/log": "^3.0",
        "symfony/console": "^6|^7",
        "nette/php-generator": "^4.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^11",
        "friendsofphp/php-cs-fixer": "^3.52",
        "phpstan/phpstan": "^1.10",
        "wol-soft/php-json-schema-model-generator": "^0.24.0"
    },
    "autoload": {
        "psr-4": {
            "OpenApiClientGenerator\\": "src",
            "OpenApiClientGeneratorTest\\": "tests/unit"
        }
    }
}
